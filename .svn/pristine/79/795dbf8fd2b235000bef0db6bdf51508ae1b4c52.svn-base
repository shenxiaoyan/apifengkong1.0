body,table{
    font-size:12px;
}

a,a:hover,a:active,a:focus{
    text-decoration: none;
   outline:none;
   -moz-outline:none; 
}
.all_wrap2{    
    margin: 0 auto;
    width: 1200px;
    background-color: #fff;
}
.all_wrap{
    margin: 40px auto 0;
    width: 1200px;
    background-color: #fff;
    -moz-box-shadow:0px 0px 10px #666666; -webkit-box-shadow:0px 0px 10px #666666; box-shadow:0px 0px 10px #666666;
}
.header_wrap {
    background:url(../images/header_bg.png) 0 0 no-repeat;
    height:120px;
    color:white;
    padding:8px 16px  16px 0;
}

.header_wrap a {
    color:white;
}

.top_wrap{
    background:url(../images/icon.png) 0 0 no-repeat;
    padding-left:20px;
}

.menu_wrap {
    padding:8px 0  0 20px;
}

.menu_ul {
    padding:0;
    margin:0;
}
.menu_ul li {
    display: inline-block;
    text-align: center;
    padding:8px 12px 0;
    line-height: 36px;
    margin: 0 3px;
}
.menu_ul li.active {
    font-weight: bold;
}

.menu_ul li:hover {
    font-weight: bold;
}

.menu_table td{
    padding:3px 18px;
    text-align: center;
    font-size: 14px;
}

.menu_table td.active a{
    font-weight: bold;
}

.print_table td:first-child{
    padding-left:0;
}


.welcome_wrap {
    height:600px;
    background:url(../images/content_bg.jpg) 0 0 no-repeat;
    position: relative;
}

.body_wrap {
    height:600px;
    position: relative;
}

.title_wrap{
    height:72px;
    padding:20px;
    background-color: #FDF3F2;
    border-bottom:1px solid #F0EBE8;
}


.content_wrap{
    height: 540px;
    overflow-y: auto;
     padding:20px;
}

.footer_wrap{
    color:#666;
    width:100%;
    height: 30px;
    position: absolute;
    bottom:0;
    right:0;
    padding-right: 15px;
}

#detail_right .tab-container{ margin-bottom:0;}
#detail_right .tab-container .nav-tabs>li>a{ padding:6px 10px;}

.detail_item_info {
 
}

.login_input {
    height: 36px;
    line-height: 36px;
    font-size: 12px;
    background: #fff;
    border-color:#ccc;
    outline: none;
    padding: 9px 16px;
    overflow: hidden;
    border-radius: 0;
}

.login_submit {
    background-color: #41464B;
    color:#fff;
     border-radius: 0;
    border-color: #41464B;
}


#detail_header{
    height:120px;
    padding:20px 16px;
    background-color: #FCF3F2;
}

.detail_content{
    height:480px;
}

.detail_table>tbody>tr>td:first-child {
    font-weight: bold;
    text-align: right;
    padding:8px 4px;
    width:110px;
}

.detail_table>tbody>tr>th{
    text-align: right;
    padding:8px 4px;
}

.tab-content {
    height:428px;
    overflow-y:auto;
}

.text_gray {
    color:#888888;
}

.list_table td{
    color:#888888;
    padding:15px 0 !important;
}

.page_title {
    font-size:20px;
}

.list_table td.list_item_name{
    color:#000;
}

#personloan_form .form-group{
    margin-bottom: 10px;
}