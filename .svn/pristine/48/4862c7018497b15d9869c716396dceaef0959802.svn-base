<script>

    "use strict";
    app.controller("personweb_detailCtrl", [
            "$scope", "$http", "$rootScope", '$state', '$stateParams',
            function ($scope, $http, $rootScope, $state, $stateParams) {

            $scope.id = $stateParams.id;
            $scope.creditInfo = {};
            $scope.getInfo = function () {
            $http.get(window.role_dir + '/personWeb/info/' + $scope.id).then(function (resRult) {

            $scope.creditInfo = resRult.data;
            if ($scope.creditInfo.analyseIdCard) {
            $scope.creditInfo.analyseIdCard = JSON.parse($scope.creditInfo.analyseIdCard);
            }
            
            if ($scope.creditInfo.analyseDishonestBlacklist) {
            $scope.creditInfo.analyseDishonestBlacklist = JSON.parse($scope.creditInfo.analyseDishonestBlacklist);
            }
            if ($scope.creditInfo.analyseMultipleHeadLend) {
            $scope.creditInfo.analyseMultipleHeadLend = JSON.parse($scope.creditInfo.analyseMultipleHeadLend);
            }

            if ($scope.creditInfo.analyseCriminal) {
            $scope.creditInfo.analyseCriminal = JSON.parse($scope.creditInfo.analyseCriminal);
            }

            if ($scope.creditInfo.analyseCourtJudgment) {
            $scope.creditInfo.analyseCourtJudgment = JSON.parse($scope.creditInfo.analyseCourtJudgment);
            }
            console.log($scope.creditInfo.analyseCourtJudgment);


            if ($scope.creditInfo.analyseEducation) {
                $scope.creditInfo.analyseEducation = JSON.parse($scope.creditInfo.analyseEducation);
            }
            
            
            if ($scope.creditInfo.analyseWatchlist) {
            $scope.creditInfo.analyseWatchlist = JSON.parse($scope.creditInfo.analyseWatchlist);
            }



            if ($scope.creditInfo.analyseAntifraud) {
            $scope.creditInfo.analyseAntifraud = JSON.parse($scope.creditInfo.analyseAntifraud);
            }
            

            if ($scope.creditInfo.analyseZhimaScore) {
                $scope.creditInfo.analyseZhimaScore = JSON.parse($scope.creditInfo.analyseZhimaScore);
                
                if($scope.creditInfo.analyseZhimaScore.color == 'success'){
                    $scope.creditInfo.analyseZhimaScore.color_code = '#009933';
                }
                if($scope.creditInfo.analyseZhimaScore.color == 'danger'){
                    $scope.creditInfo.analyseZhimaScore.color_code = '#ff0000';
                }
                if($scope.creditInfo.analyseZhimaScore.color == 'warning'){
                    $scope.creditInfo.analyseZhimaScore.color_code = '#ff6600';
                }
                
                 if($scope.creditInfo.analyseZhimaScore.color == 'primary'){
                    $scope.creditInfo.analyseZhimaScore.color_code = '#3366CC';
                }
                
                if($scope.creditInfo.analyseZhimaScore.color == 'info'){
                    $scope.creditInfo.analyseZhimaScore.color_code = '#cccccc';
                }
                
            }


            });
            };
            $scope.getInfo();
            $scope.html2cavas = function () {
            html2canvas(document.body).then(function (canvas) {
            document.body.appendChild(canvas);
            });
            }

            $scope.printArea = function () {
            //jQuery("#print_wrap #print_intro").empty().append(jQuery("#detail_intro").html());
            //jQuery("#print_wrap #print_analyseContent").empty().append(jQuery("#analyseContent").html());
            //jQuery("#print_wrap #print_phonelogContent").empty().append(jQuery("#phonelogContent").html());
            jQuery("#print_wrap").printArea();
            }

            $scope.goForm = function (info) {
                $state.go('personweb_form', {data: info});
                }
            }]);</script>
<style>
    label {
        box-shadow: none;
    }
</style>
<div   ng-controller="personweb_detailCtrl">
    <div class="body_wrap detail_page" style="background:none;">
        <div id="detail_header">
            <div class="pull-left">
                <div ui-jq="easyPieChart" ui-options="{
                     percent: 60,
                     lineWidth: 6,
                     trackColor: '#e8eff0',
                     barColor: '{{creditInfo.analyseZhimaScore.color_code}}',
                     scaleColor: '#e8eff0',
                     size: 100,
                     lineCap: 'butt',
                     animate: 1000
                     }" class="easyPieChart" style="width: 100px; height: 100px; line-height:100px; margin-top: -8px;">
                    <div>
                        <span class="h3 m-l-sm text-{{creditInfo.analyseZhimaScore.color}}">{{creditInfo.analyseZhimaScore.result ? creditInfo.analyseZhimaScore.result:'暂无'}}</span>
                        <div class="text text-sm">芝麻分</div>
                    </div>
                    <canvas width="100" height="100"></canvas></div>
            </div>
            <div class="pull-left" id="detail_intro" style="padding:3px 0 0 20px;">
