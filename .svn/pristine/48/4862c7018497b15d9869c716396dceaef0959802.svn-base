                <span ng-show="creditInfo.sex">{{creditInfo.sex}}，</span><span ng-show="creditInfo.birth">{{creditInfo.birth}}，</span><span ng-show="creditInfo.age">{{creditInfo.age}}岁，</span><span ng-show="creditInfo.area">出生于 {{creditInfo.area}}，</span><span ng-show="creditInfo.commonAddress">地址 {{creditInfo.commonAddress}}</span><br />
                {{creditInfo.analyseEducation.result.result.college}}{{creditInfo.analyseEducation.result.result.degree}} 
                <span ng-show="creditInfo.mobileCompany">{{creditInfo.mobileCompany}} </span>{{creditInfo.mobile}}<span ng-show="creditInfo.bankCard">，银行卡号 {{creditInfo.bankCard}}</span>
                <div style="font-size:14px;">

                    <span class="label label-{{creditInfo.analyseMultipleHeadLend.color}}" ng-if="creditInfo.analyseMultipleHeadLend.color == 'success'">多头借贷（优）</span> 
                    <span class="label label-{{creditInfo.analyseMultipleHeadLend.color}}" ng-if="creditInfo.analyseMultipleHeadLend.color == 'warning'">多头借贷（警）</span> 
                    <span class="label label-{{creditInfo.analyseMultipleHeadLend.color}}" ng-if="creditInfo.analyseMultipleHeadLend.color == 'danger'">多头借贷（差）</span> 

                    <span class="label label-{{creditInfo.analyseAntifraud.color}}" ng-if="creditInfo.analyseAntifraud.color == 'success'">反欺诈分析（优）</span> 
                    <span class="label label-{{creditInfo.analyseAntifraud.color}}" ng-if="creditInfo.analyseAntifraud.color == 'warning'">反欺诈分析（警）</span> 
                    <span class="label label-{{creditInfo.analyseAntifraud.color}}" ng-if="creditInfo.analyseAntifraud.color == 'danger'">反欺诈分析（差）</span> 

                    <span class="label label-{{creditInfo.analyseCriminal.color}}" ng-if="creditInfo.analyseCriminal.color == 'success'">刑事犯罪（优）</span> 
                    <span class="label label-{{creditInfo.analyseCriminal.color}}" ng-if="creditInfo.analyseCriminal.color == 'warning'">刑事犯罪（警）</span> 
                    <span class="label label-{{creditInfo.analyseCriminal.color}}" ng-if="creditInfo.analyseCriminal.color == 'danger'">刑事犯罪（差）</span> 

                    <span class="label label-{{creditInfo.analyseCourtJudgment.color}}" ng-if="creditInfo.analyseCourtJudgment.color == 'warning'">法院判决（警） </span>
                    <span class="label label-{{creditInfo.analyseCourtJudgment.color}}" ng-if="creditInfo.analyseCourtJudgment.color == 'danger'">法院判决（差）</span>
                    <span class="label label-{{creditInfo.analyseCourtJudgment.color}}" ng-if="creditInfo.analyseCourtJudgment.color == 'success'">法院判决（优）</span>


                    <span class="label label-{{creditInfo.analyseDishonestBlacklist.color}}" ng-if="creditInfo.analyseDishonestBlacklist.color == 'warning'">失信黑名单（警）</span>
                    <span class="label label-{{creditInfo.analyseDishonestBlacklist.color}}" ng-if="creditInfo.analyseDishonestBlacklist.color == 'danger'">失信黑名单（差）</span>
                    <span class="label label-{{creditInfo.analyseDishonestBlacklist.color}}" ng-if="creditInfo.analyseDishonestBlacklist.color == 'success'">失信黑名单（优）</span>




                </div>

            </div>
            <div class="pull-right" style="text-align: right">
