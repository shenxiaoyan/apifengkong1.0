                                                                            <td> {{value.disrupt_type}}</td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>执行字号</td>
                                                                            <td> {{value.code}}</td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>姓名</td>
                                                                            <td> {{value.name}}，{{value.age}}，{{value.sex}}</td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>身份证号</td>
                                                                            <td> {{value.idcardno}}</td>
                                                                        </tr>

                                                                        <tr>
                                                                            <td>公告日期</td>
                                                                            <td> {{value.pub_time}}</td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>法院</td>
                                                                            <td> {{value.court}}</td>
                                                                        </tr>

                                                                        <tr>
                                                                            <td>地区</td>
                                                                            <td> {{value.area}}</td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>执行情况</td>
                                                                            <td> {{value.performance}}</td>
                                                                        </tr>

                                                                    </tbody>
                                                                </table>
                                                        <hr>
                                                    </div>
                                                </div> 
                                                <div  ng-if="creditInfo.analyseDishonestBlacklist.color == 'success'">
                                                    {{creditInfo.analyseDishonestBlacklist.msg}}
                                                </div>
                                            </tab>

                                          
                                            <tab>
                                                <tab-heading>
                                                    <span class="text-{{creditInfo.analyseAntifraud.color}}">反欺诈</span>
                                                </tab-heading>

                                                <div  ng-if="creditInfo.analyseAntifraud">
