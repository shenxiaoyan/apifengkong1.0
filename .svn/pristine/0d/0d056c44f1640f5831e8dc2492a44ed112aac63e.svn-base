<script>

    "use strict";
    app.controller("enterpriseIntelligenceCtrl", [
        '$scope','$http',
        function ($scope,$http) {

            $scope.totalMoney = 0;
            $scope.risksList = [

                {
                    id: "",
                    name: "企业法院判决简项",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "企业法院失信简项",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "企业工商信息查询",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "商户账单真伪验证",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "POS易交账动",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "企业法院失信详情(dev)",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },

                {
                    id: "",
                    name: "企业裁判文书详情(dev)",
                    price: "￥1.00",
                    active: false,
                    resultActive:false

                },

                {
                    id: "",
                    name: "企业借贷信息(dev)",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "企业不良信息(dev)",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                },
                {
                    id: "",
                    name: "银联注册信息检索(dev)",
                    price: "￥1.00",
                    active: false,
                    resultActive:false
                }
            ];
            $scope.info = {
                entInfo: "",//企业法院判决简项全名
                company: "",//工商名称
                creditno:'',//统一信用代码
                regno:'',//注册号'

                name:'',//商户账单真伪验证商户名
                merchantNo: '',//商户账单真伪验证商户编号

                legalName:'',//商户负责人姓名
                regNo:'',//营业执照注册编号
                cellphone:'',//商户负责人手机号码
                idcard:''//商户负责人身份证号码


            }


            //企业工商信息查询方式
            $scope.enterprise=[
                {
                    type:'按企业工商名称查',
                    value:'1'
                },
                {
                    type:'按企业工商注册号查',
                    value:'2'
                },
                {
                    type:'按统一社会信用代码查',
                    value:'3'
                }
            ];
            //商户账单真伪验证
            $scope.enterprise2=[
                {
                    type:'按商户名称查',
                    value:'1'
                },
                {
                    type:'按商户编号查',
                    value:'2'
                }
            ];


            $scope.resultSwitch = false;


            $scope.Allchoice = false;

            $scope.allSelect = function () {
                $scope.totalMoney = 0
                if (!$scope.Allchoice) {
                    angular.forEach($scope.risksList, function (risk) {
                        risk.active = false
                    })

                    $scope.totalMoney = 0
                } else {
                    angular.forEach($scope.risksList, function (risk) {
                        risk.active = true
                        $scope.totalMoney += parseInt(risk.price.substr(1))
                    })

                }
            }

            $scope.selectOne = function (active) {
                if (!active) {
                    $scope.Allchoice = false

                } else {
                    var flag = false
                    angular.forEach($scope.risksList, function (risk) {
                        if (!risk.active) {
                            flag = true
                            $scope.Allchoice = false
                        }
                    })
                    if (!flag) {
                        $scope.Allchoice = true

                    }

                }
                calTotalMoney()

            }
            function calTotalMoney() {
                $scope.totalMoney = 0
                angular.forEach($scope.risksList, function (risk) {
                    if (risk.active) {
                        $scope.totalMoney += parseInt(risk.price.substr(1))
                    }
                })
            }


            //  提交
            $scope.submitInfo = function (){
                if (myForm.entInfo) {
                    myForm.entInfo.$dirty = true;

                }
                if (!myForm.$valid) {
                    $scope.resultSwitch = false;
                    return
                }


                angular.forEach($scope.risksList, function (risk,index) {
                    if (risk.active) {
                        risk.resultActive = true;
                        $scope.resultSwitch = true;

                        //企业法院判决简项0
                        if (index === 0) {
                            $http({
                                method: 'POST',
                                url: 'http://www.jinrongfengkong.com/encredit/courtJudgment',
                                data:$scope.info
                            }).then(function successCallback(response) {
                                if(response){
                                    if(response.data.data.statusCode === '2012'){
                                        $scope.dataMsg0=true;//查询成功
                                        $scope.otherMsg0=false;
                                        $scope.resultMsg0 = response.data.data.result;
                                    }
                                    else {
                                        $scope.dataMsg0=false;
                                        $scope.otherMsg0=true;//查询无数据
                                        $scope.resultMsg0 = response.data.data.statusMsg
                                    }
                                    $scope.resultSwitch = true;
                                }
                            });
                        }
                        //企业法院失信简项1
                        if (index === 1) {
                            $http({
                                method: 'POST',
                                url: 'http://www.jinrongfengkong.com/encredit/courtDishonesty',
                                data:$scope.info
                            }).then(function successCallback(response) {
                                if(response){
                                    if(response.data.data.statusCode === '2012'){
                                        $scope.dataMsg1=true;//查询成功
                                        $scope.otherMsg1=false;
                                        $scope.resultMsg1 = response.data.data.result.record;
                                    }
                                    else {
                                        $scope.dataMsg1=false;
                                        $scope.otherMsg1=true;//查询无数据
                                        $scope.resultMsg1 = response.data.data.statusMsg
                                    }
                                    $scope.resultSwitch = true;
                                }
                            });
                        }
                        //企业工商信息查询2
                        if (index === 2) {
                            $http({
                                method: 'POST',
                                url: 'http://www.jinrongfengkong.com/enterprisetrue/query',
                                data:$scope.info
                            }).then(function successCallback(response) {
                                if(response){
                                    if(response.data.status === '0'){
                                        $scope.dataMsg2=true;//查询成功
                                        $scope.otherMsg2=false;
                                        $scope.resultMsg2 = response.result;
                                    }
                                    else {
                                        $scope.dataMsg2=false;
                                        $scope.otherMsg2=true;//查询无数据
                                        $scope.resultMsg2 = response.data.msg
                                    }
                                    $scope.resultSwitch = true;
                                }


                            });
                        }
                        //商户账单真伪验证3
                        if (index === 3) {
                            $scope.resultSwitch = true;
                            $http({
                                method: 'POST',
                                url: 'http://www.jinrongfengkong.com/encredit/bill',
                                data:$scope.info
                            }).then(function successCallback(response) {
                                if(response){
                                    if(response.data.data.statusCode === '2012'){
                                        $scope.dataMsg3=true;//查询成功
                                        $scope.otherMsg3=false;
                                        $scope.resultMsg3 = response.data.data.result.record;
                                    }
                                    else {
                                        $scope.dataMsg3=false;
                                        $scope.otherMsg3=true;//查询无数据
                                        $scope.resultMsg3 = response.data.data.statusMsg
                                    }
                                    $scope.resultSwitch = true;
                                }

                            });
                        }
                        //POS易交账动4
                        if (index === 4) {
                            $http({
                                method: 'POST',
                                url: 'http://www.jinrongfengkong.com/enterpise/midReport',
                                data:$scope.info
                            }).then(function successCallback(response) {
                                if(response){
                                    if(response.data.data.statusCode === '2012'){
                                        $scope.dataMsg4=true;//查询成功
                                        $scope.otherMsg4=false;
                                        $scope.resultMsg4 = response.data.data.result.record;
                                    }
                                    else {
                                        $scope.dataMsg4=false;
                                        $scope.otherMsg4=true;//查询无数据
                                        $scope.resultMsg4 = response.data.data.statusMsg
                                    }
                                    $scope.resultSwitch = true;
                                }
                            });
                        }
                    }
                })
            }






        }
    ]);


</script>
<div class="hbox hbox-auto-xs bg-light" ng-controller="enterpriseIntelligenceCtrl">
    <!-- column -->
    <div class="col col-sm-9" style="padding: 0">
        <div class="vbox">
            <!--<div class="wrapper-md bg-light lter b-b clearfix">-->
            <!--<div class="font-thin h4">个人智能查询</div>-->
            <!--</div>-->
            <div class="row-row">
                <div class="cell">
                    <div class="cell-inner">
                        <div class="wrapper-md">
                            <div class="panel panel-info input-panel" style="margin-bottom: 20px">
                                <div class="panel-heading wrapper" style="font-size: 16px">个人身份核查</div>
                                <div class="panel-body">
                                    <form name="myForm" class="form-horizontal">
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label label1"><span style="color: #ff0000">*</span>企业名/注册号/统一社会信用代码:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.entInfo" name="entInfo"  class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.entInfo.$dirty && myForm.entInfo.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.entInfo.$error.required">企业名不能为空</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-if="YourOption.value.value==1">
                                            <label class="col-sm-2 control-label label1"><span style="color: #ff0000">*</span>企业名称:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.company" name="company" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.company.$dirty && myForm.company.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.company.$error.required">所填内容不能为空</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-if="YourOption.value.value==2">
                                            <label class="col-sm-2 control-label label1"><span style="color: #ff0000">*</span>企业工商注册号:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.regno" name="regno" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.regno.$dirty && myForm.regno.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.regno.$error.required">所填内容不能为空</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-if="YourOption.value.value==3">
                                            <label class="col-sm-2 control-label label1"><span style="color: #ff0000">*</span>统一社会信用代码:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.creditno" name="creditno" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.creditno.$dirty && myForm.creditno.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.creditno.$error.required">所填内容不能为空</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[2].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>查询方式:</label>
                                            <div class="col-sm-4">
                                                <select ng-model="YourOption.value" class="form-control" ng-options="enterpriseType.type for enterpriseType in enterprise">
                                                    <!--<option value="name">按企业名称查</option>-->
                                                    <!--<option value="regNo">按企业工商注册号查</option>-->
                                                </select>
                                                <!--<input id="queryMidValue" type="hidden" class="type" value="regNo">-->
                                            </div>
                                        </div>

                                        <div class="form-group" ng-if="YourOption2.value.value==1 || risksList[4].active">
                                            <label class="col-sm-2 control-label label1"><span style="color: #ff0000">*</span>商户名称:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.name" name="name" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.name.$dirty && myForm.name.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.name.$error.required">所填内容不能为空</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-if="YourOption2.value.value==2 || risksList[4].active">
                                            <label class="col-sm-2 control-label label1"><span style="color: #ff0000">*</span>商户编号:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.merchantNo" name="merchantNo" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.merchantNo.$dirty && myForm.merchantNo.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.merchantNo.$error.required">所填内容不能为空</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[3].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>查询方式:</label>
                                            <div class="col-sm-4">
                                                <select ng-model="YourOption2.value" class="form-control" ng-options="enterpriseType2.type for enterpriseType2 in enterprise2">
                                                </select>
                                                <input id="queryMidValue" type="hidden" class="type" value="regNo">
                                            </div>
                                        </div>
                                        <div ng-show="risksList[3].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>交易起始日期:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.beginDate" name="beginDate" class="form-control" ng-required="true" placeholder="格式yyyy-mm-dd" pattern="((((19|20)\d{2})-(0?(1|[3-9])|1[012])-(0?[1-9]|[12]\d|30))|(((19|20)\d{2})-(0?[13578]|1[02])-31)|(((19|20)\d{2})-0?2-(0?[1-9]|1\d|2[0-8]))|((((19|20)([13579][26]|[2468][048]|0[48]))|(2000))-0?2-29))$">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align: left" ng-show="myForm.beginDate.$dirty && myForm.beginDate.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.beginDate.$error.required">日期不能为空</span>
                                                <span style="color: #ff0000" ng-show="myForm.beginDate.$error.pattern">日期格式不正确</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[3].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>交易结束日期:</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.endDate" name="endDate" class="form-control" ng-required="true" placeholder="格式yyyy-mm-dd" pattern="((((19|20)\d{2})-(0?(1|[3-9])|1[012])-(0?[1-9]|[12]\d|30))|(((19|20)\d{2})-(0?[13578]|1[02])-31)|(((19|20)\d{2})-0?2-(0?[1-9]|1\d|2[0-8]))|((((19|20)([13579][26]|[2468][048]|0[48]))|(2000))-0?2-29))$">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align: left" ng-show="myForm.endDate.$dirty && myForm.endDate.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.endDate.$error.required">日期不能为空</span>
                                                <span style="color: #ff0000" ng-show="myForm.endDate.$error.pattern">日期格式不正确</span>
                                            </div>
                                            <div ng-if="info.endDate < info.beginDate" class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align: left">
                                                <span style="color: #ff0000">交易结束日期要大于交易起始日期</span>
                                            </div>
                                            <div ng-if="resultFound!=''" class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align: left" ng-show="errorResult">
                                                <span style="color: #ff0000">{{resultFound}}</span>
                                            </div>

                                        </div>

                                        <div ng-show="risksList[4].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>负责人姓名：</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.legalName" name="legalName"  class="form-control" ng-required="true" pattern="[\u4E00-\u9FA5]{2,5}(?:·[\u4E00-\u9FA5]{2,5})*">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.legalName.$dirty&&myForm.legalName.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.legalName.$error.required">姓名不能为空</span>
                                                <span style="color: #ff0000" ng-show="myForm.legalName.$error.pattern">姓名格式错误</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[4].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>负责人手机号：</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.cellphone" name="cellphone" class="form-control" ng-required="true" pattern="^1[3|4|5|7|8][0-9]{9}$">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.cellphone.$dirty && myForm.cellphone.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.cellphone.$error.required">手机不能为空</span>
                                                <span style="color: #ff0000" ng-show="myForm.cellphone.$error.pattern">手机号格式错误</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[4].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>负责人身份证：</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.idcard" name="idcard" class="form-control" ng-required="true" pattern="(^\d{15}$)|(^\d{17}([0-9]|X)$)">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.idcard.$dirty && myForm.idcard.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.idcard.$error.required">身份证号不能为空</span>
                                                <span style="color: #ff0000" ng-show="myForm.idcard.$error.pattern">身份证号格式错误</span>
                                            </div>
                                        </div>
                                        <div ng-show="risksList[4].active" class="form-group">
                                            <label class="col-sm-2 control-label"><span style="color: #ff0000">*</span>注册编号：</label>
                                            <div class="col-sm-4">
                                                <input type="text" ng-model="info.regNo" name="regNo" class="form-control" ng-required="true">
                                            </div>
                                            <div class="col-sm-offset-4 col-sm-8 col-md-offset-0 col-md-3 control-label padder" style="text-align:left" ng-show="myForm.regNo.$dirty && myForm.regNo.$invalid">
                                                <span style="color: #ff0000" ng-show="myForm.regNo.$error.required">注册编号不能为空</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-4 col-md-offset-2 col-sm-8"
                                                 style="padding-left: 20px">
                                                <button type="button" class="btn btn-info" ng-click="submitInfo()">提交</button>
                                                <button type="button" class="btn btn-info">重置</button>
                                                <button type="button" class="btn btn-danger"><i
                                                        class="fa fa-file-pdf-o"></i> 生成PDF
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!--样例-->
                            <div class="panel panel-info input-panel" ng-show="!resultSwitch">
                                <div class="panel-heading">
                                    <span style="color: #474747;">查询结果</span>
                                    <span style="color: red;">样例</span>
                                </div>
                                <div>
                                    <table class="table-border-style">
                                        <tr class="tableBg"><th style="width: 20%;">查询项名称</th><th style="width: 15%;">时间范围</th><th style="width: 25%;">说明</th><th style="width: 20%;">查询结果</th></tr>
                                        <tr><td class="S5266">商户经营城市</td><td>近12月</td><td>商户经营城市（按消费人群来 源确定）</td><td>BD98</td></tr>
                                        <tr><td class="S5132">每月还贷能力估计</td><td>近12月</td><td>根据商户每月经营状况，评估 不同行业商户的还贷能力</td><td>BD98</td></tr>
                                        <tr><td class="S5102">商户编号（MID）</td><td>近12月</td><td>商户编号（MID）信息</td><td>BD98</td></tr>
                                        <tr><td class="S5104">对应类别：（例如批发类）</td><td>近12月</td><td>商户对应类别信息</td><td>21</td></tr>
                                        <tr><td class="S5105">手续费率： %</td><td>近12月</td><td>商户手续费率信息</td><td>5411_0.38%</td></tr>
