server.port=80
spring.datasource.url = jdbc:mysql://localhost:3306/apifengkong?characterEncoding=UTF8&useSSL=false&autoReconnect=true&failOverReadOnly=false&maxReconnects=10
spring.datasource.username = root
spring.datasource.password = root


server.tomcat.maxHttpHeaderSize=90000
server.tomcat.accesslog.directory=C:\\access_logs
server.tomcat.accesslog.enabled=true
server.tomcat.accesslog.pattern=common

# Specify the DBMS

# Show or not log for each sql query
spring.jpa.show-sql = true
# Hibernate ddl auto (create, create-drop, update)
spring.jpa.hibernate.ddl-auto = update

# Enable template caching.
spring.thymeleaf.cache=true
spring.resources.cachePeriod=0
